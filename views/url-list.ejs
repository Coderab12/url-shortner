<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="css/urls-list.css" />
  </head>
  <body>
    <% if (locals.urls) { %>
    <div class="table">
      <table>
        <tr>
          <th>S.no</th>
          <th>originalUrl</th>
          <th>shortUrl</th>
        </tr>
        <% urls.forEach((element,index) => { %>
        <tr>
          <td><%= index+1 %></td>
          <td><%= urls[index].originalUrl %></td>
          <td><%= urls[index].shortUrl %></td>
        </tr>
        <% }) %>
      </table>
    </div>
    <% } %>
  </body>
</html>
